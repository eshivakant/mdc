{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Information",
      "Newtonsoft": "Information"
    }
  },
  "spring": {
    "application": { "name": "Agent-Portal-Api-Gateway" }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": false,
      "validateCertificates": false
    }
  },
  "AppSettings": {
    "AllowedOrigins": [ "http://localhost:8080", "http://localhost" ],
    "Key": "THIS_SHOULD_BE_KEPT_AT_A_SECRET_PLACE"
  },
  "AllowedHosts": "*"
}
